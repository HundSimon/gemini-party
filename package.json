{
  "name": "gemini-pool",
  "module": "index.ts",
  "dependencies": {
    "hono": "^4.7.5",
    "@google/genai": "^0.7.0",
    "openai": "^4.92.0"
  },
  "peerDependencies": {
    "typescript": "^5.8.2"
  },
  "private": true,
  "scripts": {
    "build": "bun build ./src/index.ts --outfile ./dist/bundle.js --target node --external hono --external openai --external @google/genai",
